# Manus LLM API Key (OpenAI GPT-4o compatible)
# ManusサーバーのOpenAI互換APIキーを設定
OPENAI_API_KEY=your_api_key_here

# API Base URL for Manus Server
# Manusサーバーの場合はそのエンドポイントに変更
OPENAI_BASE_URL=https://api.openai.com/v1

# Server Port
PORT=3001

# Environment (development or production)
NODE_ENV=development
